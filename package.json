{
  "name": "iconfinder",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hofens/iconfinder.git"
  },
  "bugs": {
    "url": "https://github.com/hofens/iconfinder/issues"
  },
  "dependencies": {
    "cra-template": "1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "5.4.0",
    "react-scripts": "5.0.1",
    "sharp": "0.33.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron:build": "npm run build && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.yourname.iconfinder",
    "files": [
      "build/**/*",
      "main.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "electron": "25.9.8",
    "electron-builder": "^24.6.0"
  }
}
